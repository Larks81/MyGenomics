<script>


    var listId = 287;
    $('#dialog-form').dialog({
        autoOpen: false,
        width: 350,
        show: {
            effect: "blind",
            duration: 1000
        },
        hide: {
            effect: "explode",
            duration: 1000
        },
        buttons: {
            "Iscriviti": function () {
                var email = $("#emailnl").val();
                var name = $("#namenl").val();
                $.ajax({
                    url: "/proxy.php",
                    dataType: 'json',
                    data: { act: 'addNewsletter', list: listId, email: email, name: name },
                    success: function (data) {
                        if (data.status == "KO") {
                            alert(data.msg);
                        } else {
                            alert("Grazie per esserti iscritto");
                            $("#dialog-form").dialog("close");
                        }
                    },
                    error: function () {
                        alert("Chiamata fallita!!!");
                    }
                });
            }
        }
    });


    $('.newsletter').click(function () {
        $("#dialog-form").dialog("open");
    });

</script>

