    <div id="dialog-form" title="Iscriviti alla Newsletter">
        <form>
            <fieldset>
                <label style="display:block; float:left; width:70px; margin-bottom:10px;" for="name">Nome</label>
                <input type="text" name="name" id="namenl" class="text ui-widget-content ui-corner-all" />
                <div class="clear"></div>
                <label style="display:block; float:left; width:70px;" for="email">Email</label>
